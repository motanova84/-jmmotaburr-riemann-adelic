{
  "name": "riemann-static-viewer",
  "version": "1.0.0", 
  "description": "The Definitive Proof of the Riemann Hypothesis - Static Interactive Verification Dashboard",
  "type": "module",
  "scripts": {
    "build": "mkdir -p docs && cp riemann_viewer.html docs/index.html && cp -r data docs/",
    "predeploy": "npm run build",
    "test": "python -m pytest tests/ -v",
    "validate": "python validate_explicit_formula.py",
    "setup": "python setup_environment.py"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/motanova84/-jmmotaburr-riemann-adelic.git"
  },
  "keywords": [
    "riemann-hypothesis",
    "mathematics",
    "number-theory",
    "zeta-function",
    "explicit-formula",
    "critical-line",
    "adelic",
    "static-webapp",
    "github-pages"
  ],
  "author": "José Manuel Mota Burruezo <motanova84@gmail.com>",
  "contributors": [
    {
      "name": "Instituto Conciencia Cuántica",
      "email": "contact@icq.institute"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/motanova84/-jmmotaburr-riemann-adelic/issues"
  },
  "homepage": "https://motanova84.github.io/-jmmotaburr-riemann-adelic/",
  "engines": {
    "python": ">=3.8.0"
  },
  "config": {
    "precision": "15",
    "max_zeros": "100",
    "max_primes": "100"
  }
}